<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <body>
    <%-include('partials/admin-nav'); %>
    <a class="btn btn-secondary "  href="/add-category" style="margin: 50px 35px;
    background: red;">Add Category</a>
    <%=message%>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">Id</th>
            <th scope="col">Cateogry Name</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
          <% allCategory.forEach(function (category) { %>
              <tr id="cat_<%= category.id %>">
                <td><%= category.id %></td>
                <td><%= category.name %></td>
                <td><a href="/edit-category/<%=category.id%>" class="btn btn-primary">Edit</a> <a href="/delete-category/<%=category.id%>" class="btn btn-danger">Delete</a> </td>
                </tr>
                <%});%>
          
        </tbody>
      </table>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>